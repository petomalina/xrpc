{
  "name": "xrpc-example",
  "options": {
    "allow-unauthenticated": true,
    "memory": "512Mi",
    "cpu": "1"
  },
  "build": {
    "skip": true
  },
  "hooks": {
    "prebuild": {
      "commands": [
        "export GOOGLE_RUNTIME=go",
        "export GOOGLE_BUILDABLE=./examples/grpc-http-pubsub",
        "pack build gcr.io/petermalina/xrpc --path . --builder gcr.io/buildpacks/builder:v1"
      ]
    }
  }
}