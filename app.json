{
  "name": "xrpc-example",
  "options": {
    "allow-unauthenticated": true,
    "memory": "512Mi",
    "cpu": "1"
  },
  "build": {
    "skip": true
  },
  "hooks": {
    "prebuild": {
      "commands": [
        "pack build -e GOOGLE_RUNTIME=go -e GOOGLE_BUILDABLE=./examples/grpc-http-pubsub gcr.io/petermalina/xrpc --path . --builder gcr.io/buildpacks/builder:v1"
      ]
    }
  }
}